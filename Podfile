# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'BTCTicket' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BTCTicket
  pod 'SwiftLint'
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'Action'
  pod 'RxSwiftExt'

  target 'BTCTicketTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
  end

end
